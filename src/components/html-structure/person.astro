---
// Frontmatter script in Astro
const people = [
  {
    name: 'Lindsay Walton',
    role: 'Front-end Developer',
    imageUrl: 'https://images.unsplash.com/photo-1517841905240-472988babdf9?ixlib=rb-1.2.1&ixid=eyJhcHBfaWQiOjEyMDd9&auto=format&fit=facearea&facepad=8&w=1024&h=1024&q=80',
    twitterUrl: '#',
    linkedinUrl: '#',
  },
  // More people...
];
---

<h2>People</h2>
<ul>
  {people.map(person => (
    <li key={person.name}>
      <img src={person.imageUrl} alt="" />
      <h3>{person.name}</h3>
      <p>{person.role}</p>
      <ul>
        <li>
          <a href={person.twitterUrl}>
            <span>Twitter</span>
          </a>
        </li>
        <li>
          <a href={person.linkedinUrl}>
            <span>LinkedIn</span>
          </a>
        </li>
      </ul>
    </li>
  ))}
</ul>
