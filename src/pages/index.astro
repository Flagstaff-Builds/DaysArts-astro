            <dl class="mt-10 max-w-xl space-y-8 text-base leading-7 text-gray-600 lg:max-w-none">

              {features.map((feature) => (
                <div key={feature.name} class="relative pl-9">
                  <dt class="inline font-semibold text-gray-900">
                    <span class="absolute left-1 top-1 h-5 w-5 text-indigo-600" aria-hidden="true">
                      {feature.icon}
                    </span>
                    {feature.name}
                  </dt>
                  <dd class="inline">{feature.description}</dd>
                </div>
              ))}

            </dl>
          </div>
        </div>
        <div class="flex items-start justify-end lg:order-first">
          <Image src={Hero5}  alt=""  class="w-[48rem] max-w-none rounded-xl shadow-xl ring-1 ring-gray-400/10 sm:w-[57rem]" width="2432" height="1442" />
              <div class="pointer-events-none absolute inset-0 rounded-xl ring-1 ring-inset ring-gray-900/10" />
        </div>
      </div>
    </div>
  </div>
  </Main>



</Layout>