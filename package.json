{
  "name": "astro-vue-decapcms",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "NODE_OPTIONS='--max-old-space-size=8192 --expose-gc' ASTRO_TELEMETRY_DISABLED=1 astro dev",
    "start": "NODE_OPTIONS='--max-old-space-size=8192 --expose-gc' ASTRO_TELEMETRY_DISABLED=1 astro dev",
    "build": "astro check && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "clean": "rm -rf dist .astro node_modules/.vite",
    "movie:list": "tsx scripts/updateMovieData.ts list",
    "movie:search": "tsx scripts/updateMovieData.ts search",
    "movie:update": "tsx scripts/updateMovieData.ts update"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/mdx": "^4.3.0",
    "@astrojs/sitemap": "^3.4.1",
    "@astrojs/tailwind": "^6.0.2",
    "@astrojs/vue": "^5.1.0",
    "@headlessui/vue": "^1.7.23",
    "@heroicons/vue": "^2.2.0",
    "@sentry/astro": "^9.30.0",
    "@spotlightjs/astro": "^3.1.0",
    "@tailwindcss/forms": "^0.5.10",
    "astro": "^5.9.3",
    "date-fns": "^4.1.0",
    "esbuild": "^0.25.1",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.8.2",
    "vite": "^6.3.5",
    "vue": "3.5.13"
  },
  "devDependencies": {
    "@babel/helpers": "^7.27.0",
    "@tailwindcss/typography": "^0.5.16",
    "glob": "^11.0.2",
    "gray-matter": "^4.0.3",
    "tsx": "^4.19.4",
    "yaml": "^2.8.0"
  }
}
